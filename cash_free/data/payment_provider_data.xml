<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="payment_acquirer_cashfree" model="payment.acquirer">
        <field name="name">Cash Free</field>
        <field name="provider">cashfree</field>
        <field name="image_128" type="base64" file="cash_free/static/description/icon.png"/>
        <field name="cashfree_app_id">dummy</field>
        <field name="cashfree_secret_key">dummy</field>
        <field name="cashfree_test_url">https://sandbox.cashfree.com/pg/links</field>
        <!-- <field name="cashfree_production_url">https://api.cashfree.com/pg/links</field> -->
        <field name="cashfree_api_version">2022-09-01</field>
        <field name="redirect_form_view_id" ref="redirect_form"/>
    </record>


    <record id="payment_method_cashfree" model="account.payment.method">
        <field name="name">Cashfree</field>
        <field name="code">cashfree</field>
        <field name="payment_type">inbound</field>
    </record>

